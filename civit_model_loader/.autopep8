[autopep8]
max_line_length = 120
# CRITICAL: Do NOT break f-strings or any string literals
# Ignore ALL line-related errors to prevent string breaking
ignore = E501,W503,E704,E127,E128,E124,E125,E126,E129,E122,E123,E133,E226,E241,E242
aggressive = 0
experimental = false
hang_closing = false
# ABSOLUTELY prevent ALL line breaking that could affect strings
# Do NOT use --wrap-descriptions or any line wrapping
